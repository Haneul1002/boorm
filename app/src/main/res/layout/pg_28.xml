<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_times_solid"
    tools:context=".pg_28_java">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="54dp"
        android:layout_height="61dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="176dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="29dp"
        android:background="#00F6F3F3"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/angle_left_solid"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="405dp"
        android:layout_height="85dp"
        android:layout_marginBottom="16dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.975">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="109dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:id="@+id/RelativeLayout01"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:background="@drawable/border_top"
            android:nestedScrollingEnabled="false">

            <Button
                android:id="@+id/button18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="16dp"
                android:height="75dp"
                android:background="@drawable/border_topbot"
                android:gravity="left|center_vertical"
                android:onClick="btnOnClick1"
                android:text="(이벤트) 카풀 이용하고 스탬프 받자!"
                android:textColor="#090909"
                android:textSize="18dp"

                />


            <Button
                android:id="@+id/button19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/button18"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:height="75dp"
                android:background="@drawable/border_topbot"
                android:gravity="left|center_vertical"
                android:text="매칭이 완료되었습니다!"
                android:textColor="#090909"
                android:textSize="18dp"
                android:onClick="btnOnClick2"/>

            <Button
                android:id="@+id/button20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/button19"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="21dp"
                android:height="75dp"
                android:background="@drawable/border_topbot"
                android:gravity="left|center_vertical"
                android:text="(공지사항) V1.5 업데이트 내역"
                android:textColor="#090909"
                android:textSize="18dp"
                android:onClick="btnOnClick3"/>

            <Button
                android:id="@+id/button24"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignStart="@+id/button18"
                android:layout_alignTop="@+id/button18"
                android:layout_marginStart="373dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/ic_times_solid"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <Button
                android:id="@+id/button25"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignStart="@+id/button19"
                android:layout_alignTop="@+id/button19"
                android:layout_marginStart="373dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/ic_times_solid"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <Button
                android:id="@+id/button26"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignStart="@+id/button20"
                android:layout_alignTop="@+id/button20"
                android:layout_marginStart="373dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/ic_times_solid"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        </RelativeLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>